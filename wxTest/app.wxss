/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
}

.fl {
  float: left;
}

.clearfix {
  clear: both;
}

.clearfix:after {
  display: table;
  content: "";
  clear: both;
}

.clearfix:before {
  display: table;
  content: "";
}

.limitOne {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
